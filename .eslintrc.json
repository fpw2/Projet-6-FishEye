{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "plugins": ["prettier"],
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "prettier/prettier": ["error",{
            "singleQuote": true
        }]
    }
}
